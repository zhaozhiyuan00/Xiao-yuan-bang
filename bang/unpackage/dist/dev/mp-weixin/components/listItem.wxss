
.re-list {
	width: 100%;
	height: 100rpx;
	position: relative;
}
.re-list:active {
	background-color: rgba(0,0,0,.1);
}
.re-list image {
	width: 52rpx;
	height: 52rpx;
	margin-left: 20rpx;
}
.re-list text:first-of-type {
	margin-left: 14rpx;
}
.re-list .fa-angle-right {
	position: absolute;
	right: 20rpx;
	font-size: 40rpx;
}

